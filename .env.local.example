# Dashboard Environment Configuration - Direct Auth Fix
# Copy this to .env.local for local development
# For production, set these in Netlify environment variables

# DIRECT SUPABASE AUTH (bypasses broken api.lanonasis.com)
VITE_SUPABASE_URL=https://<project-ref>.supabase.co
VITE_SUPABASE_ANON_KEY=REDACTED_SUPABASE_ANON_KEY
VITE_ONASIS_SUPABASE_ANON_KEY=REDACTED_SUPABASE_ANON_KEY

# Authentication Mode
VITE_USE_DIRECT_AUTH=true
VITE_USE_CENTRAL_AUTH=false
VITE_PROJECT_SCOPE=dashboard

# Service URLs
VITE_DASHBOARD_URL=https://dashboard.lanonasis.com
VITE_DOCS_URL=https://docs.lanonasis.com
VITE_MCP_URL=https://mcp.lanonasis.com

# Optional: Central Auth (only if you fix api.lanonasis.com)
# VITE_API_URL=https://api.lanonasis.com/v1
# VITE_AUTH_GATEWAY_URL=https://api.lanonasis.com